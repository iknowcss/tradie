{
  "name": "tradie",
  "version": "0.1.6",
  "keywords": [
    "tradie", "build", "tool", "bundle", "bundler", "lint", "test", "browserify", "sass-composer", "eslint", "mocha"
  ],
  "description": "An opinionated build tool for frontend projects. Use it to lint, bundle and test your script and style files.",
  "repository": {
    "type": "git",
    "url": "git@github.com:jameslnewell/tradie.git"
  },
  "dependencies": {
    "autoprefixer": "^6.2.3",
    "browserify": "^12.0.1",
    "browserify-incremental": "^3.0.1",
    "chalk": "^1.1.1",
    "clean-css": "^3.4.8",
    "commander": "^2.9.0",
    "envify": "^3.4.0",
    "eslint": "^1.10.3",
    "file-size": "^1.0.0",
    "humanize-duration": "^3.5.0",
    "mkdirp": "^0.5.1",
    "mocaccino": "^1.8.2",
    "postcss": "^5.0.13",
    "promised-del": "^1.0.2",
    "promisepipe": "^1.0.1",
    "rc": "^1.1.6",
    "sass-composer": "^2.0.0-beta12",
    "source-map-support": "^0.4.0",
    "source-mapper": "^2.0.0",
    "watchify": "^3.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-preset-es2015": "^6.5.0"
  },
  "bin": {
    "tradie": "dist/bin/cli.js",
    "tradie-init": "dist/bin/cli-init.js",
    "tradie-clean": "dist/bin/cli-clean.js",
    "tradie-lint": "dist/bin/cli-lint.js",
    "tradie-bundle-scripts": "dist/bin/cli-bundle-scripts.js",
    "tradie-bundle-styles": "dist/bin/cli-bundle-styles.js",
    "tradie-bundle": "dist/bin/cli-bundle.js",
    "tradie-build": "dist/bin/cli-build.js",
    "tradie-test": "dist/bin/cli-test.js"
  },
  "scripts": {
    "build": "babel --debug --out-dir dist src",
    "prepublish": "npm run build"
  },
  "license": "MIT"
}
